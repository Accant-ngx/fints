{
    "name": "node-fints",
    "version": "0.3.5",
    "description": "FinTS client library.",
    "keywords": [
        "fints",
        "hbci",
        "banking",
        "api",
        "ios",
        "nodejs"
    ],
    "scripts": {
        "prebuild": "cxsd -t generated https://raw.githubusercontent.com/hbci4j/hbci4java/master/src/main/resources/pain.001.001.03.xsd",
        "build": "tsc -p ./tsconfig-build.json",
        "clean": "rm -rf dist",
        "docs": "typedoc --mode file --out docs/ src/",
        "test": "TZ=UTC jest"
    },
    "files": [
        "dist",
        "LICENSE",
        "package.json",
        "README.md"
    ],
    "main": "dist/index.js",
    "repository": "https://github.com/guidoMueller/nodejs-fints",
    "typings": "dist/index.d.ts",
    "author": "Guido MÃ¼ller",
    "license": "MIT",
    "jest": {
        "collectCoverage": true,
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "transform": {
            ".ts": "ts-jest"
        },
        "coverageReporters": [
            "lcov",
            "text-summary"
        ],
        "testMatch": [
            "**/__tests__/test-*.ts"
        ],
        "globals": {
            "ts-jest": {
                "enableTsDiagnostics": true,
                "ignoreCoverageForDecorators": true,
                "ignoreCoverageForAllDecorators": true
            }
        },
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/__tests__/"
        ],
        "collectCoverageFrom": [
            "src/**/*.ts"
        ]
    },
    "devDependencies": {
        "@types/date-fns": "^2.6.0",
        "@types/fetch-mock": "^7.3.1",
        "@types/iconv-lite": "^0.0.1",
        "@types/invariant": "^2.2.30",
        "@types/isomorphic-fetch": "^0.0.35",
        "@types/jest": "^24.0.18",
        "@types/node": "^12.7.7",
        "@types/winston": "^2.4.4",
        "cxsd": "^0.1.1",
        "fetch-mock": "^7.3.9",
        "jest": "^24.9.0",
        "ts-jest": "^24.1.0",
        "tslint": "^5.20.0",
        "typedoc": "^0.15.0",
        "typescript": "^3.6.3"
    },
    "dependencies": {
        "bind-decorator": "^1.0.11",
        "date-fns": "^2.3.0",
        "fast-xml-parser": "^3.12.20",
        "iconv-lite": "^0.5.0",
        "invariant": "^2.2.4",
        "isomorphic-fetch": "^2.2.1",
        "mt940-js": "^0.6.0",
        "winston": "^3.2.1"
    }
}
